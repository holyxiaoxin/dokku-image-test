<p id="notice"><%= notice %></p>

<h1>Mid Scores</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mid_scores.each do |mid_score| %>
      <tr>
        <td><%= mid_score.game %></td>
        <td><%= mid_score.score %></td>
        <td><%= link_to 'Show', mid_score %></td>
        <td><%= link_to 'Edit', edit_mid_score_path(mid_score) %></td>
        <td><%= link_to 'Destroy', mid_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mid Score', new_mid_score_path %>
